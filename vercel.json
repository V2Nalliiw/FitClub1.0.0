{
  "rewrites": [
    {
      "source": "/settings(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/preferences(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/profile(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
